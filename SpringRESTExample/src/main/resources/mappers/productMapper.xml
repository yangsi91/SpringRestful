<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kitchen.mapper.ProductMapper">
	<select id="getProductId" resultType="com.kitchen.model.Product">
		select *
		from product
		where name=#{name};
	</select>
</mapper>