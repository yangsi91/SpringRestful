<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kitchen.mapper.CookMapper">
	<select id="getListByRecipeId" resultType="com.kitchen.model.Cook">
		select *
		from cook
		where recipe_id=#{recipe_id};
	</select>
</mapper>